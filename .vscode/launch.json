{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Markvis",
      "program": "${workspaceFolder}/markvis/debug.js",
      "cwd": "${workspaceFolder}/markvis",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "JS prettify",
      "program": "${workspaceFolder}/js/debug.js",
      "cwd": "${workspaceFolder}/js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Jupyter",
      "program": "${workspaceFolder}/jupyter/render.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
      "cwd": "${workspaceFolder}/jupyter"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\test.js"
    }
  ]
}